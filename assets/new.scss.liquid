.tabbed {
    margin-top: 88px;
    margin-bottom: 86px;
    @include clearfix;

    @include media($mobile) {
        margin-top: 16px;
        margin-bottom: 32px;
    }

    .tabbed-inner {
        @include grid-column(8);
        @include grid-shift(2);

        @include media($mobile) {
            @include grid-column(12);
            @include grid-shift(0);
        }
    }

    .tabs {
        height: 40px;

        border-bottom: none;
        border-left: none;
        @include clearfix;
        border-radius: 2px;
        cursor: pointer;

        .tab {
            height: 40px;
            line-height: 40px;
            width: 33.333%;
            border-right: none;
            float: left;
            box-sizing: border-box;
            text-align: center;
            cursor: pointer;

            padding: 0px 2.5px;

            .title {
                line-height: 40px;
                text-align: center;
                display: block;
                box-sizing: border-box;
                border: 1px solid #DCDCDC;
                transition: $transition;
                color: black;
                @extend .pragmatica-medium;
                font-size: 12px;
                letter-spacing: 1px;
            }

            @include media($mobile) {
                padding: 0px;
                .title {
                    border-left: none;
                }


                &:first-child {
                    .title {
                        border-left: 1px solid #DCDCDC;
                    }
                }
            }

            @include media($desktop) {
                &:hover {
                    .title {
                        background-color: black;
                        color: white;
                        border-color: black;
                    }
                }
            }

            &.active {
                .title {
                    background-color: black;
                    color: white;
                    border-color: black;
                }
                position: relative;
            }
        }
    }

    .tab-content-wrap {
        position: relative;
        height: 264px;
        overflow: hidden;
        transition: $transition;
    }

    .tab-content {
        position: absolute;
        top: 0px;
        left: 0px;
        width: 100%;
        opacity: 0;

        height: 264px;
        transition: $transition;
        border-top: none;
        border-bottom-left-radius: 2px;
        border-bottom-right-radius: 2px;
        box-sizing: border-box;
        box-sizing: border-box;
        overflow: hidden;
        padding-bottom: 0px;

        &.active {
            opacity: 1;
            z-index: 5;
        }

        &.expanded {
            overflow-y: scroll;
        }

        .actual-content {
            box-sizing: border-box;
            padding: 32px 16px;
            padding-bottom: 23px;

            @include media($mobile) {
                padding-left: 0px;
                padding-right: 0px;
            }

            .container {
                width: auto !important;
            }

            strong {
                @extend .pragmatica-medium;
                font-size: 11px;
                color: #1F1E1E;
                letter-spacing: 0.5px;
                line-height: 16px;
            }

            h3 {
                @extend .pragmatica-medium;
                text-transform: uppercase;
                font-size: 10px;
                color: #1F1E1E;
                letter-spacing: 1px;
                line-height: 10px;
                margin-top: 0px;
                margin-bottom: 13px;
            }

            p {
                font-size: 14px;
                color: #1F1E1E;
                letter-spacing: 0.5px;
                line-height: 20px;
                margin-top: 0px;
                margin-bottom: 20px;
            }
        }



        .expand {
            background-image: linear-gradient(-180deg, rgba(255,255,255,0.00) 0%, #FFFFFF 100%);
            position: absolute;
            width: 100%;
            bottom: 0px;
            left: 0px;
            height: 50%;
            text-align: center;
            box-sizing: border-box;
            display: none;

            .button {
                height: 40px;
                line-height: 34px;
                width: 208px;
                box-sizing: border-box;
                position: absolute;
                top: calc(50% - 20px);
                left: calc(50% - 104px);
                transition: $transition;

                color: black;
                background: white;
                border-color: #DCDCDC;
                border-radius: 0px;
                @extend .pragmatica-medium;
                font-size: 12px;
                letter-spacing: 1px;
                text-shadow: none;

                @include media($desktop) {
                    &:hover {
                        background: black;
                        color: white;
                        border-color: black;
                    }
                }
            }
        }
    }
}

.pro-low {
    border: none;
    margin: 0px;
    padding: 0px;
    margin-top: 18px;

    &.desktop {
        display: block;
        @include media($mobile) {
            display: none;
        }
    }

    &.mobile {
        display: none;
        @include media($mobile) {
            display: block;
        }
    }

    @include media($mobile) {
        padding-left: 16px;
        padding-right: 16px;
    }

    h6 {
        font-size: 12px;
        font-weight: bold;
        letter-spacing: 1px;
        line-height: 17px;
        margin: 0px;
        margin-bottom: 16px;
    }

    li {
        font-size: 12px;
        font-weight: 300;
        letter-spacing: 1px;
        line-height: 17px;
        padding-left: 24px;

        &.shipping {
            @include retina('app', 16px 16px);
        }

        &.delivery {
            @include retina('cart-return', 15px 16px);
            background-position: 0px 0px;
        }

        &.talk {
            @include retina('single-04', 14px 16px);
            background-position: 1px 0px;
        }

        &.price {
            @include retina('currency-dollar', 8px 16px);
            background-position: 3px 0px;
        }
    }

    .price span {
        background: white;
        border: none;
        padding: 0px;
        color: black;
        margin: 0px !important;
        margin-left: 15px !important;
        display: inline-block;

        &:hover {
            background: white;
            border: none;
            padding: 0px;
            color: black;
        }

        a {
            font-size: 10px;
            font-weight: bold;
            letter-spacing: 1px;
            line-height: 14px;
            border-bottom: solid 2px black;
            color: black !important;
            transition: $transition;

            @include media($desktop) {
                &:hover {
                    border-color: white;
                }
            }
        }
    }
}

.best-seller {
    margin-bottom: 64px;

    @include media($mobile) {
        margin-bottom: 0px;
    }

    h2 {
        text-transform: none;
        margin: 0px;
        padding: 0px;
        text-align: center;
        margin-bottom: 28px;
        @extend .pragmatica-medium;
        font-size: 15px;
        color: #000000;
        letter-spacing: 0;
        line-height: 21px;
    }
}

.snize-ac-results {
