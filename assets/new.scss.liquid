.hamburger {
    vertical-align: top;
    padding-top: 5px;
    cursor: pointer;
    margin-left: 12px;

    .l {
        width: 24px;
        display: block;
        height: 2px;
        background: white;
        border-radius: 1px;
        margin-bottom: 6px;
        margin-top: 6px;
        transition: $transition;
        opacity: 1;
        position: relative;
    }

    display: none;

    @include media($mobile) {
        display: inline-block;
    }

    &.active {
        .l2 {
            opacity: 0;
        }

        .l1 {
            transform: rotate(-45deg);
            top: 8px;
        }

        .l3 {
            transform: rotate(45deg);
            top: -8px;
        }
    }
}

